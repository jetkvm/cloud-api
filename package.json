    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.19",
    "@types/node": "^24.9.0",
    "@types/ws": "^8.18.1",
    "cookie-session": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
